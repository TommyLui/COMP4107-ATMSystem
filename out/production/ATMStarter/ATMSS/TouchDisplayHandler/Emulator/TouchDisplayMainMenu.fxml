<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>

<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Text?>
<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="520.0"
            prefWidth="680.0" xmlns="http://javafx.com/javafx/8.0" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ATMSS.TouchDisplayHandler.Emulator.TouchDisplayEmulatorController">
    <children>
        <AnchorPane prefHeight="480.0" prefWidth="640.0" AnchorPane.leftAnchor="20" AnchorPane.topAnchor="20">
            <children>
                <Rectangle arcHeight="5.0" arcWidth="5.0" fill="TRANSPARENT" height="480.0"
                           onMouseClicked="#td_mouseClick" stroke="BLACK" strokeType="OUTSIDE" width="640"
                           AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.topAnchor="0"/>

                <StackPane AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" onMouseClicked="#td_checkBalance">
                    <Rectangle arcHeight="5.0" arcWidth="5.0" fill="white" height="70.0" stroke="BLACK"
                               strokeType="OUTSIDE" width="300.0"/>
                    <Text>Account balance enquiry</Text>
                </StackPane>

                <StackPane AnchorPane.bottomAnchor="0" AnchorPane.rightAnchor="0" onMouseClicked="#td_changePin">
                    <Rectangle arcHeight="5.0" arcWidth="5.0" fill="white" height="70.0" stroke="BLACK"
                               strokeType="OUTSIDE"
                               width="300.0"/>
                    <Text>Change pin</Text>
                </StackPane>

                <StackPane AnchorPane.bottomAnchor="70" AnchorPane.leftAnchor="0" onMouseClicked="#td_deposit">
                    <Rectangle arcHeight="5.0" arcWidth="5.0" fill="white" height="70.0" stroke="BLACK" strokeType="OUTSIDE"
                               width="300.0" />
                    <Text>Deposit</Text>
                </StackPane>

                <StackPane AnchorPane.bottomAnchor="70" AnchorPane.rightAnchor="0" onMouseClicked="#td_withdrawal">
                    <Rectangle arcHeight="5.0" arcWidth="5.0" fill="white" height="70.0" stroke="BLACK" strokeType="OUTSIDE"
                               width="300.0" />
                    <Text>WithDrawal</Text>
                </StackPane>

                <StackPane AnchorPane.bottomAnchor="140" AnchorPane.rightAnchor="0" onMouseClicked="#td_ejectCard">
                <Rectangle arcHeight="5.0" arcWidth="5.0" fill="white" height="70.0" stroke="BLACK" strokeType="OUTSIDE"
                           width="300.0" AnchorPane.bottomAnchor="140" AnchorPane.rightAnchor="0"/>
                    <Text>Eject card</Text>
                </StackPane>


                <Rectangle arcHeight="5.0" arcWidth="5.0" fill="white" height="70.0" stroke="BLACK" strokeType="OUTSIDE"
                           width="300.0" AnchorPane.bottomAnchor="70" AnchorPane.rightAnchor="0"/>

                <Rectangle arcHeight="5.0" arcWidth="5.0" fill="white" height="70.0" stroke="BLACK" strokeType="OUTSIDE"
                           width="300.0" AnchorPane.bottomAnchor="140" AnchorPane.leftAnchor="0"/>


                <!--				<Rectangle arcHeight="5.0" arcWidth="5.0" fill="TRANSPARENT" height="480.0"-->
                <!--						   onMouseClicked="#td_mouseClick" stroke="BLACK" strokeType="OUTSIDE" width="640"-->
                <!--						   AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.topAnchor="0"/>-->
            </children>
        </AnchorPane>
    </children>
</AnchorPane>

